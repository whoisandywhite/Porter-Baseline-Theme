{
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json",
	"settings": {
		"appearanceTools": true,
		"border": {
			"radius": false
		},
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"gradients": [],
			"palette": [
				{
					"color": "#FFFFFF",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#121212",
					"name": "Contrast",
					"slug": "contrast"
				}
			]
		},
		"layout": {
			"contentSize": "50rem",
			"wideSize": "84rem"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "Tiny",
					"size": "0.5rem",
					"slug": "tiny"
				},
				{
					"name": "Small",
					"size": "1rem",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "2rem",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "3rem",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "4rem",
					"slug": "x-large"
				},
				{
					"name": "Extra Extra Large",
					"size": "5rem",
					"slug": "xx-large"
				},
				{
					"name": "Global Spacing",
					"size": "clamp( 2rem, 7vw, 5.5rem)",
					"slug": "global"
				}
			],
			"units": [
				"px",
				"em",
				"rem",
				"%"
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [],
			"fontSizes": [
				{
					"fluid": {
						"max": "14px",
						"min": "10px"
					},
					"name": "Tiny",
					"size": "14px",
					"slug": "tiny"
				},
				{
					"fluid": {
						"max": "16px",
						"min": "12px"
					},
					"name": "Small",
					"size": "16px",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "20px",
						"min": "16px"
					},
					"name": "Body",
					"size": "20px",
					"slug": "body"
				},
				{
					"fluid": {
						"max": "26px",
						"min": "20px"
					},
					"name": "Regular",
					"size": "26px",
					"slug": "regular"
				},
				{
					"fluid": {
						"max": "28px",
						"min": "18px"
					},
					"name": "Medium",
					"size": "28px",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "40px",
						"min": "24px"
					},
					"name": "Large",
					"size": "40px",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "48px",
						"min": "29px"
					},
					"name": "Display 1",
					"size": "48px",
					"slug": "display-1"
				}
			],
			"writingMode": true
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--regular)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--body)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--body)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--body)"
				}
			},
			"heading": {
				"typography": {
					"fontStyle": "normal",
					"fontWeight": "700",
					"lineHeight": "1.2"
				}
			},
			"button": {
				"border": {
					"radius": "0"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--body)",
					"fontWeight": "400"
				},
				"spacing": {
					"padding": {
						"top": "0.35em",
						"right": "1.25em",
						"bottom": "0.35em",
						"left": "1.25em"
					}
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				":hover" : {
					"color": {
						"text": "var(--wp--preset--color--contrast)"
					},
					"typography": {
						"fontWeight": "700"
					}
				}
			}
		},
		"spacing": {
			"blockGap": "clamp( 1em, 3vw, 2em)",
			"padding": {
				"left": "var(--wp--preset--spacing--global)",
				"right": "var(--wp--preset--spacing--global)"
			}
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--body)",
			"lineHeight": "1.4"
		}
	},
	"version": 3
}